{
  "expo": {
    "name": "Dinge",
    "slug": "dinge",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ff5a60"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff5a60"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A GPS-based, highly localized platform to connect people and the community."
  }
}
